{
  "name": "agence-adeliom/easy-faq-bundle",
  "type": "symfony-bundle",
  "description": "Symfony bundle for EasyAdmin",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Adeliom\\EasyFaqBundle\\": "src/"
    }
  },
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "symfony/config": "^4.0|^5.0",
    "symfony/dependency-injection": "^4.0|^5.0",
    "symfony/form": "^4.0|^5.0",
    "symfony/framework-bundle": "^4.0|^5.0",
    "symfony/uid": "^5.3",
    "ramsey/uuid-doctrine": "^1.7",
    "symfony/http-foundation": "^4.0|^5.0",
    "symfony/http-kernel": "^4.0|^5.0",
    "symfony/options-resolver": "^4.0|^5.0",
    "symfony/property-access": "^4.0|^5.0",
    "symfony/routing": "^4.0|^5.0",
    "symfony/twig-bundle": "^4.0|^5.0",
    "symfony/webpack-encore-bundle": "^1.12",
    "babdev/pagerfanta-bundle": "^2.10",
    "pagerfanta/twig": "^2.7",
    "pagerfanta/doctrine-orm-adapter": "^2.7"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.14",
    "symfony/framework-bundle": "^4.0",
    "symfony/phpunit-bridge": "^4.1"
  },
  "scripts": {
    "cs-fixer": "vendor/bin/php-cs-fixer fix -v --dry-run",
    "cs-fixer-fix": "vendor/bin/php-cs-fixer fix -v"
  }
}
